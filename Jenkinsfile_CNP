#!groovy

@Library("Infrastructure")

def type = "nodejs"
def product = "labs"
def component = "louisehuyton-walkthrough"

def yarnBuilder = new uk.gov.hmcts.contino.YarnBuilder(this)

withPipeline(type, product, component) {
  after('build') {
    yarnBuilder.yarn('build')
  }

}

after('Smoke Test - AKS sandbox') {
    steps.archiveArtifacts allowEmptyArchive: true, artifacts: './smoke-output/reports/DefaultPageTemplate-result.xml'
  }
